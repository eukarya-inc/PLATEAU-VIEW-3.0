{
  "name": "reearth-plugin-plateauview",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "./dist/reearth-plugin-plateauview.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start:datacatalog": "vite -c vite.config.datacatalog-web.ts",
    "start:sidebar": "vite -c vite.config.sidebar-web.ts",
    "build": "run-s typecheck build:web build:plugin",
    "build:plugin": "run-s build:sidebar build:datacatalog",
    "build:datacatalog": "vite build -c vite.config.datacatalog.ts",
    "build:sidebar": "vite build -c vite.config.sidebar.ts",
    "build:web": "run-s build:datacatalog-web build:sidebar-web",
    "build:datacatalog-web": "vite build -c vite.config.datacatalog-web.ts",
    "build:sidebar-web": "vite build -c vite.config.sidebar-web.ts",
    "typecheck": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "jsdom": "^19.0.0",
    "msw": "^0.41.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-singlefile": "^0.9.0",
    "vitest": "^0.12.9"
  },
  "msw": {
    "workerDirectory": "web"
  }
}
