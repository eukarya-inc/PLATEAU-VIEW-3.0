{
  "name": "reearth-plugin-plateauview",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "./dist/reearth-plugin-plateauview.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start:sidebar": "vite -c vite-configs/sidebar/web.ts",
    "start:location": "vite -c vite-configs/location/web.ts",
    "start:datacatalog": "vite -c vite-configs/sidebar/modals/datacatalog/web.ts",
    "start:welcomescreen": "vite -c vite-configs/sidebar/modals/welcomescreen/web.ts",
    "start:google": "vite -c vite-configs/location/modals/googleAnalytics/web.ts",
    "start:terrain": "vite -c vite-configs/location/modals/terrain/web.ts",
    "build": "run-s typecheck clean build:plugin",
    "build:plugin": "run-s build:sidebar build:location build:geolocation",
    "build:sidebar": "run-s build:datacatalog-web build:welcomescreen-web; vite build -c vite-configs/sidebar/web.ts; vite build -c vite-configs/sidebar",
    "build:location": "run-s build:google-web build:terrain-web build:location-web; vite build -c vite-configs/location",
    "build:geolocation": "run-s build:geolocation-web;vite build -c vite-configs/geolocation",
    "build:location-web": "vite build -c vite-configs/location/web.ts",
    "build:google-web": "vite build -c vite-configs/location/modals/googleAnalytics/web.ts",
    "build:terrain-web": "vite build -c vite-configs/location/modals/terrain/web.ts",
    "build:datacatalog-web": "vite build -c vite-configs/sidebar/modals/datacatalog/web.ts",
    "build:welcomescreen-web": "vite build -c vite-configs/sidebar/modals/welcomescreen/web.ts",
    "build:geolocation-web": "vite build -c vite-configs/geolocation/web.ts",
    "zip": "node ./zip.mjs",
    "clean": "del dist",
    "clean:web": "del dist/web",
    "typecheck": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/remarkable": "2.0.3",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "3.0.1",
    "antd": "4.22.8",
    "archiver": "5.3.1",
    "del-cli": "5.0.0",
    "eslint": "8.31.0",
    "eslint-config-reearth": "0.2.1",
    "jsdom": "21.0.0",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "msw": "0.49.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "rollup-plugin-visualizer": "5.9.0",
    "styled-components": "5.3.6",
    "typescript": "4.9.4",
    "typescript-styled-plugin": "0.18.2",
    "vite": "4.0.4",
    "vite-plugin-cdn-import": "0.3.5",
    "vite-plugin-dts": "1.7.1",
    "vite-plugin-singlefile": "0.13.2",
    "vite-plugin-svgr": "2.4.0",
    "vitest": "0.27.0",
    "yml": "1.0.0"
  },
  "dependencies": {
    "@types/react-leaflet": "2.8.2",
    "jotai": "1.12.1",
    "leaflet": "1.9.2",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-accessible-accordion": "5.0.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.1.0",
    "remarkable": "2.0.1"
  },
  "msw": {
    "workerDirectory": "web/mock"
  }
}
